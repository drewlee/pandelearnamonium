{
  "name": "pandelearnamonium-algorhythmics",
  "version": "1.0.0",
  "description": "TypeScript algorhythmics.",
  "private": true,
  "author": "Andrew A. Lee",
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "build": "tsc -b",
    "eslint": "eslint .",
    "fcc": "npm run ts-node ./scripts/fcc-file-formatter.ts ./freecodecamp-problems",
    "fcc:clean": "rm -rf ./freecodecamp-problems/*.txt",
    "lint": "npm run prettier:write && npm run eslint",
    "lint:check": "npm run prettier:check && npm run eslint",
    "prepare": "cd .. && husky",
    "prettier": "prettier . -w",
    "prettier:check": "prettier . -c",
    "prettier:write": "npm run prettier",
    "start": "echo 'running start'",
    "test": "npm run lint:check && npm run build && tsc -b ./tests && vitest run",
    "ts-node": "node --import ./ts-node-esm-loader.js --no-deprecation"
  },
  "lint-staged": {
    "*": [
      "prettier -uw",
      "eslint"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/node": "^22.13.5",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-tsdoc": "^0.4.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "3.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0",
    "vitest": "^4.0.18"
  },
  "volta": {
    "node": "24.13.0",
    "npm": "11.10.1"
  }
}
